    {
      "cell_type": "code",
      "source": [
        "\n",
        "\n",
        "import feedparser\n",
        "import folium\n",
        "import pandas as pd\n",
        "import time\n",
        "from IPython.display import display, HTML\n",
        "from datetime import datetime\n",
        "\n",
        "# ---------------- CONFIG ----------------\n",
        "cities = {\n",
        "    \"Gaza\": {\"lat\": 31.5017, \"lon\": 34.4668, \"aliases\": [\"gaza\", \"gaza city\", \"gaza strip\"]},\n",
        "    \"Beirut\": {\"lat\": 33.8938, \"lon\": 35.5018, \"aliases\": [\"beirut\", \"lebanon\"]},\n",
        "    \"Damascus\": {\"lat\": 33.5138, \"lon\": 36.2765, \"aliases\": [\"damascus\", \"syria\"]},\n",
        "    \"Northern Israel Border\": {\"lat\": 33.27, \"lon\": 35.57, \"aliases\": [\"northern israel\", \"israel-lebanon border\", \"galilee\", \"border with lebanon\"]}\n",
        "}\n",
        "\n",
        "rss_feeds = [\n",
        "    \"https://www.aljazeera.com/xml/rss/all.xml\",\n",
        "    \"http://feeds.bbci.co.uk/news/world/middle_east/rss.xml\",\n",
        "    \"https://rss.dw.com/rdf/rss-en-all\",\n",
        "    \"http://rss.cnn.com/rss/edition_world.rss\",\n",
        "    \"http://feeds.reuters.com/Reuters/worldNews\"\n",
        "]\n",
        "\n",
        "risk_keywords = [\n",
        "    'attack','explosion','war','strike','missile','death',\n",
        "    'protest','conflict','rocket','terror','bomb','hostage',\n",
        "    'shooting','shelling','air raid'\n",
        "]\n",
        "\n",
        "# ---------------- HELPERS ----------------\n",
        "def safe_parse(url):\n",
        "    try:\n",
        "        return feedparser.parse(url)\n",
        "    except Exception as e:\n",
        "        print(\"Feed parse error:\", url, e)\n",
        "        return None\n",
        "\n",
        "def classify_risk(score):\n",
        "    if score >= 8:\n",
        "        return \"HIGH\"\n",
        "    if score >= 4:\n",
        "        return \"MEDIUM\"\n",
        "    if score > 0:\n",
        "        return \"LOW\"\n",
        "    return \"NONE\"\n",
        "\n",
        "def fetch_risk_data(cities_info, feeds, keywords, max_headlines=5):\n",
        "    counts = {c: 0 for c in cities_info}\n",
        "    headlines = {c: [] for c in cities_info}\n",
        "    for url in feeds:\n",
        "        parsed = safe_parse(url)\n",
        "        if not parsed or not hasattr(parsed, \"entries\"):\n",
        "            continue\n",
        "        for entry in parsed.entries:\n",
        "            title = (entry.get(\"title\") or \"\").strip()\n",
        "            summary = (entry.get(\"summary\") or entry.get(\"description\") or \"\").strip()\n",
        "            text = (title + \" \" + summary).lower()\n",
        "            pub = entry.get(\"published\", entry.get(\"updated\", \"N/A\"))\n",
        "            link = entry.get(\"link\", \"\")\n",
        "            for city, info in cities_info.items():\n",
        "                if any(alias in text for alias in info.get(\"aliases\", [])):\n",
        "                    kcount = sum(1 for k in keywords if k in text)\n",
        "                    counts[city] += (kcount if kcount > 0 else 1)\n",
        "                    label = f\"{pub} | {title}\"\n",
        "                    if label not in [h[0] for h in headlines[city]]:\n",
        "                        headlines[city].append((pub, title, link))\n",
        "                        headlines[city] = headlines[city][:max_headlines]\n",
        "        # polite pause so feeds are not hammered\n",
        "        time.sleep(0.25)\n",
        "    return counts, headlines\n",
        "\n",
        "# ---------------- RUN FETCH ----------------\n",
        "print(\"Fetching RSS feeds (this can take 10-30s)...\")\n",
        "counts, headlines = fetch_risk_data(cities, rss_feeds, risk_keywords)\n",
        "print(\"Scores:\", counts)\n",
        "\n",
        "# ---------------- BUILD MAP ----------------\n",
        "m = folium.Map(location=[33.5, 35.4], zoom_start=6, tiles='CartoDB positron')\n",
        "color_map = {\"HIGH\":\"darkred\", \"MEDIUM\":\"orange\", \"LOW\":\"blue\", \"NONE\":\"green\"}\n",
        "\n",
        "for city, info in cities.items():\n",
        "    lat, lon = info[\"lat\"], info[\"lon\"]\n",
        "    score = counts.get(city, 0)\n",
        "    level = classify_risk(score)\n",
        "    color = color_map[level]\n",
        "    radius = 6 + score * 2\n",
        "\n",
        "    # popup: show risk + top headlines\n",
        "    popup_html = f\"<b>{city}</b><br><b>Risk:</b> {level} (score {score})<br><hr>\"\n",
        "    if headlines.get(city):\n",
        "        for pub, title, link in headlines[city]:\n",
        "            safe_title = title if title else \"(no title)\"\n",
        "            if link:\n",
        "                popup_html += f\"<a href='{link}' target='_blank'>{safe_title[:120]}</a><br>\"\n",
        "            else:\n",
        "                popup_html += f\"{safe_title[:120]}<br>\"\n",
        "    else:\n",
        "        popup_html += \"No recent headlines.<br>\"\n",
        "\n",
        "    folium.CircleMarker(\n",
        "        location=(lat, lon),\n",
        "        radius=radius,\n",
        "        color=color,\n",
        "        fill=True,\n",
        "        fill_color=color,\n",
        "        fill_opacity=0.7,\n",
        "        popup=folium.Popup(popup_html, max_width=400),\n",
        "        tooltip=f\"{city}: {level} ({score})\"\n",
        "    ).add_to(m)\n",
        "\n",
        "# ---------------- SHOW MAP INLINE (Colab-friendly) ----------------\n",
        "# Using folium's HTML representation and display() so Colab renders it inline reliably\n",
        "display(HTML(m._repr_html_()))\n",
        "print(\"Map displayed above. Scroll up to interact with it.\")\n"
      ],
